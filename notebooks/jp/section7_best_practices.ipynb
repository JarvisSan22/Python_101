    "# 良い例\n",
    "def calculate_fish_weight(length, species):\n",
    "    if species == \"マグロ\":\n",
    "        return length ** 2 * 0.5\n",
    "    elif species == \"サケ\":\n",
    "        return length * 0.8\n",
    "    else:\n",
    "        return length * 0.6\n",
    "\n",
    "# 悪い例\n",
    "def calculate_fish_weight_bad(length,species):\n",
    "  if species==\"マグロ\": return length**2*0.5\n",
    "  elif species==\"サケ\":return length*0.8\n",
    "  else:return length*0.6"
   ]
