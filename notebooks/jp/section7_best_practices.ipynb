  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import timeit\n",
    "\n",
    "# 測定する関数\n",
    "def traditional_loop():\n",
    "    result = []\n",
    "    for i in range(1000):\n",
    "        result.append(i * i)\n",
    "    return result\n",
    "\n",
    "def list_comprehension():\n",
    "    return [i * i for i in range(1000)]\n",
    "\n",
    "# 実行時間の測定\n",
    "traditional_time = timeit.timeit(traditional_loop, number=1000)\n",
    "comprehension_time = timeit.timeit(list_comprehension, number=1000)\n",
    "\n",
    "print(f\"従来のループの実行時間: {traditional_time:.6f}秒\")\n",
    "print(f\"リストの内包表記の実行時間: {comprehension_time:.6f}秒\")\n",
    "print(f\"速度の比: {traditional_time / comprehension_time:.2f}倍\")"
