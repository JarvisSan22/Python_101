    "            \"name\": self.name,\n",
    "            \"species\": self.species,\n",
    "            \"habitat\": self.habitat,\n",
    "            \"size\": self.size,\n",
    "            \"weight\": self.weight,\n",
    "            \"description\": self.description,\n",
    "            \"image_path\": self.image_path\n",
    "        }\n",
