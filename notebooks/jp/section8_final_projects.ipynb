    "# データモデルの例\n",
    "class Fish:\n",
    "    \"\"\"魚のデータモデル\"\"\"\n",
    "    \n",
    "    def __init__(self, name, species, habitat, size, weight=None, description=None, image_path=None):\n",
    "        \"\"\"\n",
    "        魚のインスタンスを初期化\n",
    "        \n",
    "        Args:\n",
    "            name (str): 魚の名前（日本語名）\n",
    "            species (str): 魚の種類（学名）\n",
    "            habitat (str): 生息地\n",
    "            size (float): 平均サイズ（cm）\n",
    "            weight (float, optional): 平均重量（kg）\n",
    "            description (str, optional): 説明\n",
    "            image_path (str, optional): 画像ファイルへのパス\n",
    "        \"\"\"\n",
    "        self.name = name\n",
    "        self.species = species\n",
    "        self.habitat = habitat\n",
    "        self.size = size\n",
    "        self.weight = weight\n",
    "        self.description = description\n",
    "        self.image_path = image_path\n",
    "    \n",
    "    def __str__(self):\n",
    "        \"\"\"魚の情報を文字列として返す\"\"\"\n",
    "        return f\"{self.name} ({self.species}) - 生息地: {self.habitat}, サイズ: {self.size}cm\"\n",
    "    \n",
    "    def to_dict(self):\n",
    "        \"\"\"魚の情報を辞書として返す\"\"\"\n",
