    "class FishRepository:\n",
    "    \"\"\"魚のデータにアクセスするためのリポジトリクラス\"\"\"\n",
    "    \n",
    "    def __init__(self, db_path):\n",
    "        \"\"\"リポジトリを初期化\"\"\"\n",
    "        self.db_path = db_path\n",
    "    \n",
    "    def _get_connection(self):\n",
    "        \"\"\"データベース接続を取得\"\"\"\n",
    "        return sqlite3.connect(self.db_path)\n",
    "    \n",
    "    def add_fish(self, fish):\n",
    "        \"\"\"魚を追加\"\"\"\n",
    "        conn = self._get_connection()\n",
    "        cursor = conn.cursor()\n",
    "        \n",
    "        cursor.execute('''\n",
    "        INSERT INTO fish (name, species, habitat, size, weight, description, image_path)\n",
    "        VALUES (?, ?, ?, ?, ?, ?, ?)\n",
    "        ''', (fish.name, fish.species, fish.habitat, fish.size, fish.weight, fish.description, fish.image_path))\n",
    "        \n",
    "        conn.commit()\n",
    "        fish_id = cursor.lastrowid\n",
    "        conn.close()\n",
    "        \n",
    "        return fish_id\n",
    "    \n",
    "    def get_all_fish(self):\n",
